{
  "name": "template",
  "version": "0.1.0",
  "description": "WEBサイト制作環境のテンプレート",
  "author": "webdev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:webdev-jp-net/template.git"
  },
  "keywords": [
    "gulp"
  ],
  "main": "gulpfile.js",
  "scripts": {
    "prettier:scss": "prettier src/**/*.scss --write",
    "prettier:js": "prettier src/**/*.js --write",
    "lint:js": "eslint --fix src/**/*.js",
    "start": "gulp"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "targets": {
            "browsers": [
              "ie >= 11",
              "iOS >= 10",
              "android >= 4.4",
              "last 1 versions"
            ]
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.3",
    "delete-empty": "^2.0.0",
    "eslint": "^5.10.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^4.0.2",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.27.1",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {}
}
